{% for label,flashes in app.session.flashbag.all %}
    {% for flash in flashes %}
        {% if(label == 'success') %}
<div class="alert alert-success" role="alert">
    {{ flash }}
</div>
{% elseif(label == 'error') %}
<div class="alert alert-danger" role="alert">
    {{ flash }}
</div>
{% endif %}
{% endfor %}
{% endfor %}